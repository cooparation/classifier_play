net: "multilabel/train_alexnet.prototxt"
test_iter: 100
#test_interval: 500
test_interval: 100
#base_lr: 0.0001
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 500
display: 20
max_iter: 10000
#max_iter: 6000
#momentum: 0.9
momentum: 0.7
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "models/multilabel_alexnet/alexnet_train"
solver_mode: GPU
